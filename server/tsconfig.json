{
  "include": [
    "./index.ts"
  ],
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es5",
      "es6",
      "dom"
    ],
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "moduleResolution": "node",
    "inlineSources": true
  }
}
